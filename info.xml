<?xml version="1.0"?>
<extension key="de.systopia.eventmessages" type="module">
  <file>eventmessages</file>
  <name>Custom Event Communication</name>
  <description>Provides a more flexible way of sending emails to event participant.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.eventmessages</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.eventmessages</url>
    <url desc="Support">https://github.com/systopia/de.systopia.eventmessages/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-09-15</releaseDate>
  <version>1.1-beta3</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.2</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Eventmessages</namespace>
  </civix>
</extension>
